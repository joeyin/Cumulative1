@model School.Models.Course
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Course/List">Back To Classes</a>
<div id="containershow">
    <h2>@Model.ClassName @Model.ClassCode</h2>
    <p>Teacher Name: <a href="/Teacher/Show/@Model.TeacherId">@Model.TeacherFName</a></p>
    <p>Start Date: @Model.StartDate</p>
    <p>Finish Date: @Model.FinishDate</p>
    <p>Class Name: @Model.ClassName</p>
</div>